<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="warehouses_seller_tiki_form_view" model="ir.ui.view">
            <field name="name">warehouses.seller.tiki.form</field>
            <field name="model">warehouses.seller.tiki</field>
            <field name="arch" type="xml">
                <form string="ModelTitle">
                    <header>
                        <button type="object" name="get_warehouses_seller_tiki" string="Tạo sản phẩm Tiki" />
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="warehouses_id_seller"/>
                            </group>
                            <group>
                                <field name="seller_id"/>
                                <field name="status"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="warehouses_seller_tiki_tree_view" model="ir.ui.view">
            <field name="name">warehouses.seller.tiki.tree</field>
            <field name="model">warehouses.seller.tiki</field>
            <field name="arch" type="xml">
                <tree string="ModelTitle">
                    <field name="name"/>
                    <field name="warehouses_id_seller"/>
                    <field name="seller_id"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>



        <record id="warehouses_seller_tiki_act_window" model="ir.actions.act_window">
            <field name="name">warehouses seller tiki</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">warehouses.seller.tiki</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="warehouses seller" id="warehouses_seller_tiki_menu" parent="product_tiki_act" action="warehouses_seller_tiki_act_window"/>

    </data>
</odoo>