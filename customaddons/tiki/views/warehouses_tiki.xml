<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="warehouses_tiki_form_view" model="ir.ui.view">
            <field name="name">warehouses.tiki.form</field>
            <field name="model">warehouses.tiki</field>
            <field name="arch" type="xml">
                <form string="ModelTitle">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="code"/>
                            </group>
                            <group>
                                <field name="warehouses_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="warehouses_tiki_tree_view" model="ir.ui.view">
            <field name="name">warehouses.tiki.tree</field>
            <field name="model">warehouses.tiki</field>
            <field name="arch" type="xml">
                <tree string="ModelTitle">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="warehouses_id"/>
                </tree>
            </field>
        </record>



        <record id="warehouses_tiki_act_window" model="ir.actions.act_window">
            <field name="name">warehouses tiki</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">warehouses.tiki</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="warehouses" id="warehouses_tiki_menu" parent="product_tiki_act" action="warehouses_tiki_act_window"/>

    </data>
</odoo>